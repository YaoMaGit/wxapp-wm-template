page {
  background: #fff;
  margin-bottom: 200rpx;
}

.top_div {
  background-image: url(data:image/png;base64,);
  width: 100%;
  height: 350rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  padding-top: 45rpx;
}

.card_div {
  width: 90%;
  margin: auto;
  height: 320rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  border-radius: 25rpx;
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
  color: #fff;
  padding: 40rpx 20rpx;
}

.card_top {
  flex: 1;
  padding-top: 25rpx;
  display: flex;
}

.card_bottom {
  flex: 1;
  display: flex;
  justify-content: space-around;
  padding-top: 30rpx;
}

.card_top_left {
  flex: 1;
}

.card_top_left>image {
  width: 85rpx;
  height: 85rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}

.card_top_center {
  flex: 6;
  padding-top: 15rpx;
}

.card_top_right {
  flex: 2;
  padding-top: 15rpx;
}

.card_top_right_top {
  /* background: rgba(255, 255, 255, 0.1); */
  border-top-left-radius: 25rpx;
  border-bottom-left-radius: 25rpx;
  
  background: linear-gradient(to right, rgba(255, 255, 255, 0.3), rgba(255, 0, 0, 0));
  /* padding: 0rpx 4rpx; */
}

.padd_top {
  display: inline-block;
  /* margin-top: 8rpx; */
  line-height: 40rpx;
}

.touying_div image {
  /* box-shadow:0px 1px 5px 0px rgba(0,0,0,0.3); */
  border-radius: 50%;
}

.card_top_right_top>.singin_img {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-right: 13rpx;
  
  border-radius: 50%;
}

.card_top_right_top>view>.right_img {
  width: 15rpx;
  height: 20rpx;
  vertical-align: middle;
  margin-left: 15rpx;
  margin-bottom: 5rpx;
}

.card_bottom>view, .card_bottom>navigator {
  flex: 1;
  text-align: center;
}
.card_top_center_top{
  font-size: 30rpx;
  font-weight: 500;
}
.card_top_center_bottom{
  padding-top: 5rpx;
  color: rgba(255, 255, 255, 0.9);
  font-size: 22rpx;
}
.card_top_right_bottom{
  padding-top: 10rpx;
  padding-left: 10rpx;
}
.num_div{
  padding-bottom: 5rpx;
}
.xxx{
  display: inline-block;
  margin-top: 10rpx;
}
/*  */

.plur_div {
  width: 100%;
  height: 210rpx;
  /* background: #CCCCCC; */
  margin-top: 120rpx;
  font-size: 24rpx;
  color: #444;
}

.plur_box {
  display: flex;
  justify-content: space-around;
}

.plur_box>view, .plur_box>navigator {
  flex: 1;
  text-align: center;
}

.item_img {
  width: 130rpx;
  height: 130rpx;
  display: block;
  margin: auto;
  margin-bottom: 10rpx;
}

/*  */

.myfx_title_div {
  border-top: 10rpx solid #efefef;
  border-bottom: 1rpx solid #efefef;
  font-size: 30rpx;
  padding: 25rpx 20rpx;
  padding-left: 40rpx;
  font-weight: 600;
}

.myfx_plur_div {
  width: 100%;
  /* height: 210rpx; *//* background: #CCCCCC; */
  font-size: 24rpx;
  color: #444;
  padding-top: 50rpx;
}

.myfx_plur_box {
  display: flex;
  justify-content: space-around;
}

.myfx_plur_box>view, .myfx_plur_box>navigator {
  flex: 1;
  text-align: center;
}

.myfx_item_img {
  width: 53rpx;
  height: 53rpx;
  display: block;
  margin: auto;
  margin-bottom: 30rpx;
}

.subtitle_div {
  display: flex;
}

.subtitle_div>.subtitle_div1 {
  flex: 2;
}

.subtitle_div>.subtitle_div2 {
  flex: 7;
}

.padding_css {
  padding-top: 45rpx;
  padding-bottom: 50rpx;
}

/*  */

.ylike_div {
  text-align: center;
  font-size: 26rpx;
  color: #444;
  background: #efefef;
}

.ylike_div>text {
  padding: 0 8rpx;
}

.eg_div {
  color: #fd6c6c;
}

.goods_item {
  display: flex;
  font-size: 22rpx;
  color: #999;
  justify-content: space-around;
  margin: 10rpx;
  margin-top: 20rpx;
}

.goods_left {
  flex: 1;
}

.goods_rigth {
  flex: 3;
  display: flex;
  flex-direction: column;
}

.goods_left>view {
  width: 144rpx;
  height: 144rpx;
  margin: 15rpx;
}

.goods_left image {
  width: 100%;
  height: 100%;
}

.goods_right {
  border-bottom: 1px solid #EEEEEE;
  padding-bottom: 15rpx;
}

.goods_right>view {
  padding-bottom: 18rpx;
}

.title_div {
  color: #444;
  font-size: 30rpx;
  font-weight: 500;
}

.price_div1 {
  color: #ff1c00;
  font-size: 32rpx;
  padding-right: 10rpx;
}

.price_div2 {
  color: #c9c9c9;
  font-size: 22rpx;
  text-decoration: line-through;
}

.xihuan-box .top {
  height: 72rpx;
  line-height: 72rpx;
  font-size: 22rpx;
  text-align: center;
}

.xihuan-box .top .line {
  display: inline-block;
  width: 80rpx;
  height: 1px;
  background: #9f9f9f;
  margin: 0 12rpx;
  margin-bottom: 8rpx;
}

.xihuan-box .top .left {
  color: #666;
}

.xihuan-box .top .right {
  color: #ff5151;
}
